[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"Tower",{"topName":"塔配置表","twoName":"塔","head":[["备注","配置备注","导出类型"],["id","ID","num"],["name","名字","string"],["quality","品质","int"],["weight","权重","int"],["icon","Icon","string"],["prefab","预制体","string"],["groundImg","占地图片","string"],["unlock","解锁章节","int"],["desc","塔简介","string"]],"data":[["弩箭塔",101,"豌豆宝",1,10,"Texture/TowerWar/Icon/ta_1_1","ArrowTower","img_jianta_jiaobiao_1",0,"释放高速豌豆，对单体目标造成致命穿透伤害；升级后可获得连射加速，瞬间压制来犯精英。"],["光束塔‌",102,"暴躁菇",3,10,"Texture/TowerWar/Icon/ta_10_1","MagicTower","img_jianta_jiaobiao_6",9,"发射连续离子光束，贯穿直线上的所有敌人；火力极致但占用地格较大，适合在要塞侧翼部署"],["旋刃符文",103,"释迦果",1,10,"Texture/TowerWar/Icon/ta_2_1","FanTower","img_jianta_jiaobiao_1",0,"召唤尖利的飞叶环绕自身高速旋转，对范围内敌军造成多段打击，持续压制接近单位"],["飞剑阵",104,"怒怒蕉",3,10,"Texture/TowerWar/Icon/ta_4_1","SwordTower","img_jianta_jiaobiao_2",3,"召唤香蕉，在敌群中来回穿梭斩击，输出灵活，适合防守路径交叉地带。"],["剧毒坩埚",105,"毒毒菇",2,10,"Texture/TowerWar/Icon/ta_3_1","PoisonTower","img_jianta_jiaobiao_3",1,"发射腐蚀毒液，持续削弱敌军生命值，对高生命单位尤为致命,造成持续伤害"],["龙息炮",106,"辣椒怒",2,10,"Texture/TowerWar/Icon/ta_5_1","FlameTower","img_jianta_jiaobiao_3",0,"喷吐火焰，灼烧前方大范围敌人，输出稳定，克制集群部队。"],["‌冰锥塔",107,"‌冰冰萝",2,10,"Texture/TowerWar/Icon/ta_6_1","IceTower","img_jianta_jiaobiao_2",0,"发射尖锐冰锥攻击敌人，并造成减速效果，是控场与输出兼顾的关键防线。"],["血蝠巢穴",108,"唤花蒲",3,10,"Texture/TowerWar/Icon/ta_8_1","BatTower","img_jianta_jiaobiao_5",5,"召唤蒲公英，持续攻击敌人目标，具备多次打击与扰乱能力。"],["电磁塔",109,"电芦苇",3,10,"Texture/TowerWar/Icon/ta_9_1","FlashTower","img_jianta_jiaobiao_4",7,"释放电能闪链，可在多个目标间跳跃打击，擅长处理成群敌人。"],["迫击火炮",110,"瓜投手",2,10,"Texture/TowerWar/Icon/ta_7_1","FireBallTower","img_jianta_jiaobiao_2",0,"投射樱桃炸弹，落地瞬间引发范围爆炸，对密集敌军造成毁灭性打击。"],["女神",114,"兔猎手",0,0,"Texture/TowerWar/King/bg_guowang_tx_1","BaseTower","img_jianta_jiaobiao_1",0,"‌兔猎手技能可以强化友军"]]}]],0,0,[],[],[]]